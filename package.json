{
  "name": "qub",
  "version": "1.0.0",
  "description": "A collection of common data structures and functions",
  "main": "output/sources/Qub.js",
  "files": [
    "output/sources/"
  ],
  "scripts": {
    "build": "tsc -p .",
    "prepublish": "npm run build",
    "test": "mocha output/tests/ --ui tdd",
    "coverage": "nyc npm test && open-cli output/coverage/index.html",
    "travis-test": "nyc npm test | coveralls"
  },
  "nyc": {
    "reporter": [
      "html",
      "text-lcov"
    ],
    "report-dir": "output/coverage/",
    "temp-directory": "output/.nyc_output/",
    "exclude": [
      "output/tests"
    ],
    "check-coverage": true,
    "functions": 100,
    "lines": 100,
    "branches": 100,
    "statements": 100
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danschultequb/qub-typescript.git"
  },
  "keywords": [
    "qub"
  ],
  "author": "Dan Schulte",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danschultequb/qub-typescript/issues"
  },
  "homepage": "https://github.com/danschultequb/qub-typescript#readme",
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "coveralls": "^2.13.1",
    "mocha": "^3.3.0",
    "nyc": "^11.0.2",
    "open-cli": "^1.0.5",
    "typescript": "^2.3.2"
  }
}
